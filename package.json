{
    "name": "framework7-react-single-view",
    "private": true,
    "version": "1.3.0",
    "description": "framework7-react-single-view",
    "repository": "",
    "license": "UNLICENSED",
    "scripts": {
        "start": "npm run dev",
        "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.js",
        "watch": "webpack-cli --watch --mode production --config ./build/webpack.config.js",
        "build": "npm run build-prod",
        "build-dev": "cross-env NODE_ENV=development node ./build/build.js",
        "build-prod": "cross-env NODE_ENV=production node ./build/build.js",
        "copy-font": "cpy ./node_modules/framework7-icons/fonts/*.* ./src/fonts/",
        "monaca:preview": "npm run dev & npm run watch",
        "monaca:transpile": "npm run build",
        "monaca:debug": "npm run watch"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-whitelist": {},
            "cordova-plugin-statusbar": {},
            "monaca-plugin-monaca-core": {}
        }
    },
    "browserslist": [
        "Android >= 7",
        "IOS >= 11",
        "Safari >= 11",
        "Chrome >= 49",
        "Firefox >= 31",
        "Samsung >= 5"
    ],
    "dependencies": {
        "cordova-plugin-splashscreen": "6.0.0",
        "cordova-plugin-whitelist": "1.3.5",
        "cordova-plugin-statusbar": "2.4.3",
        "monaca-plugin-monaca-core": "3.3.1",
        "dom7": "^2.1.5",
        "framework7": "^5.7.11",
        "framework7-icons": "^3.0.1",
        "framework7-react": "^5.7.11",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "template7": "^1.4.2"
    },
    "devDependencies": {
        "@babel/core": "^7.11.5",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.11.5",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "@babel/runtime": "^7.11.2",
        "babel-loader": "^8.1.0",
        "chalk": "^4.1.0",
        "copy-webpack-plugin": "^6.1.0",
        "cordova": "^10.0.0",
        "cpy-cli": "^3.1.1",
        "cross-env": "^7.0.2",
        "css-loader": "^4.2.2",
        "file-loader": "^6.1.0",
        "html-webpack-plugin": "^4.4.1",
        "mini-css-extract-plugin": "^0.11.0",
        "optimize-css-assets-webpack-plugin": "^5.0.4",
        "ora": "^5.0.0",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.7.0",
        "style-loader": "^1.2.1",
        "terser-webpack-plugin": "^4.1.0",
        "url-loader": "^4.1.0",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    }
}
